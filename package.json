  "repository": "https://github.com/jakec-dev/travel-planner",
  "author": "Jake Clayton <jake@jakec.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "webpack serve --env development",
    "build": "webpack",
    "start": "node server.js",
