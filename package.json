    "test:coverage": "yarn test --coverage",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint  --fix \"src/**/*.{js,jsx}\"",
    "format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format:write": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "@csstools/normalize.css": "^12.0.0",
    "express": "^4.18.1",
    "jest": "^28.1.2",
    "prop-types": "^15.8.1",
